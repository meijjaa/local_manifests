<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="ia"
           fetch="https://github.com/android-ia/" />

  <remove-project name="kernel/common" />
  <remove-project name="platform/external/mesa" />
  <remove-project name="platform/external/drm_gralloc" />
  <remove-project name="platform/external/libdrm" />
  <remove-project name="LineageOS/android_external_libncurses" />
  <remove-project name="LineageOS/android_external_powertop" />

  <project name="device-androidia-kernel" path="kernel" remote="ia" revision="master" />
  <project name="external-drm_hwcomposer" path="vendor/intel/external/android_ia/drm_hwcomposer" groups="drm_hwcomposer" remote="ia" revision="master"/>
  <project name="hwcomposer" path="vendor/intel/external/android_ia/hwcomposer" groups="hwcomposer" remote="ia" revision="master"/>
  <project name="external-libdrm" path="vendor/intel/external/android_ia/libdrm" groups="pdk" remote="ia" revision="master"/>
  <project name="external-mesa" path="external/mesa" remote="ia" revision="master" />
  <project name="external-vulkan_hal" path="vendor/intel/external/android_ia/vulkan" remote="ia" revision="master" />
  <project name="external-drm_gralloc" path="vendor/intel/external/android_ia/drm_gralloc" groups="drm_gralloc" remote="ia" revision="master"/>
  <project name="minigbm" path="vendor/intel/external/android_ia/minigbm" remote="ia" revision="master" />
  <project name="libncurses" path="external/libncurses" remote="ia" revision="master"/>
  <project name="powertop" path="external/powertop" remote="ia" revision="master"/>
  <project name="pciutils" path="external/pciutils" remote="ia" revision="master"/>
  <project name="device-androidia" path="device/intel/android_ia" remote="ia" revision="master" />
</manifest>
